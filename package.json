      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "bcrypt": "^3.0.5",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "ejs": "~2.5.7",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "jsonwebtoken": "^8.5.0",
    "mongoose": "^5.4.17",
    "morgan": "~1.9.0",
    "multer": "^1.4.1"
  },
  "devDependencies": {
    "@types/jest": "22.1.2",
    "@types/supertest": "2.0.4",
    "dotenv": "^6.2.0",
    "jest": "22.4.2",
    "nodemon": "^1.18.10",
    "supertest": "3.0.0",
    "ts-jest": "22.0.4",
    "typescript": "2.6.2"
  }
